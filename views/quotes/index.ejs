<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Quote</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% quotes.forEach(function(m) { %>
      <tr>
        <td><%= m.quote %></td>
        <td><a href="/quotes/<%= m._id %>">Thoughts</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
